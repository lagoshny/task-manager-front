{
  "name": "task-manager",
  "version": "0.0.1",
  "config": {
    "logLevel": "-LL"
  },
  "scripts": {
    "init": "npm config set loglevel silent & npm install",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=dev cross-var gulp dev $npm_package_config_logLevel --gulpfile=configs/matreshka/builds/gulpfile.js",
    "dev:aot": "cross-env NODE_ENV=dev cross-var gulp dev:aot $npm_package_config_logLevel --gulpfile=configs/matreshka/builds/gulpfile.js",
    "test": "cross-env NODE_ENV=dev cross-var gulp onlyTest $npm_package_config_logLevel --gulpfile=configs/matreshka/builds/gulpfile.js",
    "prod": "cross-env NODE_ENV=prod cross-var gulp prod $npm_package_config_logLevel --gulpfile=configs/matreshka/builds/gulpfile.js",
    "prod:aot": "cross-env NODE_ENV=prod cross-var gulp prod:aot $npm_package_config_logLevel --gulpfile=configs/matreshka/builds/gulpfile.js",
    "ngc": "ngc ./node_modules/.bin/ngc -p ./configs/matreshka/builds/tsconfig.aot.json",
    "lint:css": "cross-env NODE_ENV=dev gulp css:linting --gulpfile=configs/matreshka/builds/gulpfile.js",
    "lint:js": "cross-env NODE_ENV=dev gulp js:linting --gulpfile=configs/matreshka/builds/gulpfile.js",
    "lint:ts": "cross-env NODE_ENV=dev gulp ts:linting --gulpfile=configs/matreshka/builds/gulpfile.js",
    "clean": "gulp clean --gulpfile=configs/matreshka/builds/gulpfile.js"
  },
